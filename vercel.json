{
  "version": 2,
  "builds": [
    { "src": "Backen_Portfolio/server.js", "use": "@vercel/node" },
    { "src": "harshportfolio/package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "Backen_Portfolio/server.js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "harshportfolio/dist/index.html" }
  ]
}